             /*  Re-create objects for the specified target release  */
             /* ---------------------------------------------------- */
             RMVLIBLE   LIB(&LIB)
             MONMSG     MSGID(CPF2104)

             ADDLIBLE   LIB(&LIB) POSITION(*FIRST)

             RMVLIBLE   LIB(&DEVLIB)
             MONMSG     MSGID(CPF2104) EXEC(DO)
               CHGVAR     VAR(&IS_DEVLIB) VALUE('0')
             ENDDO

             STRREXPRC SRCMBR(A_INSTALL) SRCFILE(&LIB/QBUILD) +
                         PARM('INSTALL' *BCAT &LIB *BCAT &DBGVIEW *BCAT &TGTRLS)

             RMVLIBLE   LIB(&LIB)
             IF         COND(&IS_DEVLIB) THEN(DO)
               ADDLIBLE   LIB(&DEVLIB) POSITION(*FIRST)
             ENDDO

             /* ---------------------------------------------------- */
             /*  Change object owner to QPGMR.                       */
             /* ---------------------------------------------------- */
             CRTBNDCL   PGM(&TMPLIB/CRTPKGOWN) +
                          SRCFILE(&LIB/&QBUILD) SRCMBR(*PGM) +
                          DFTACTGRP(*NO) ACTGRP(*CALLER)

             CALL       PGM(&TMPLIB/CRTPKGOWN) PARM(&LIB &TMPLIB &OWNER)

             /* ---------------------------------------------------- */
             /*  Create target library description.                  */
             /* ---------------------------------------------------- */
             CHGVAR     VAR(&TEXT) VALUE(&OBJD *BCAT '-' *BCAT +
                                         &RELEASE *BCAT '(' *CAT &TODAY *TCAT ')')

             /* ---------------------------------------------------- */
             /*  Save install package to save file                   */
             /* ---------------------------------------------------- */
             CHGLIB     LIB(&LIB) TEXT(&TEXT)

             DLTF       FILE(&TMPLIB/&SAVF)
             MONMSG     MSGID(CPF2105)

             CRTSAVF    FILE(&TMPLIB/&SAVF) TEXT(&TEXT)

      /*        SAVLIB     LIB(&LIB) DEV(*SAVF) SAVF(&TMPLIB/&SAVF) +          */
      /*                     TGTRLS(&TGTRLS) DTACPR(*HIGH) +                   */
      /*                     OMITOBJ((&LIB/*ALL     *PGM   )  +                */
      /*                          (&LIB/*ALL     *SRVPGM)  +                   */
      /*                          (&LIB/*ALL     *MODULE)  +                   */
      /*                          (&LIB/*ALL     *CMD   )  +                   */
      /*                          (&LIB/*ALL     *DTAARA)  +                   */
      /*                          (&LIB/*ALL     *USRSPC)  +                   */
      /*                          (&LIB/*ALL     *JOBD  )  +                   */
      /*                          (&LIB/*ALL     *PNLGRP)  +                   */
      /*                          (&LIB/*ALL     *WSCST )  +                   */
      /*                          (&LIB/&SAVF    *FILE  )  +                   */
      /*                             (&LIB/EVFEVENT *FILE  ))                  */

             SAVLIB     LIB(&LIB) DEV(*SAVF) SAVF(&TMPLIB/&SAVF) +
                          TGTRLS(&TGTRLS) DTACPR(*HIGH)

             DLTF       FILE(&LIB/&SAVF)
             MONMSG     MSGID(CPF2105)

             MOVOBJ     OBJ(&TMPLIB/&SAVF) OBJTYPE(*FILE) TOLIB(&LIB)

             DLTPGM     PGM(&TMPLIB/CRTPKGOWN)
             DLTPGM     PGM(&TMPLIB/CRTPKGMDT)
             DLTPGM     PGM(&TMPLIB/CRTPKGCPR)

             SNDPGMMSG  MSGID(CPF9897) MSGF(*LIBL/QCPFMSG) +
                          MSGDTA('*** Successfully create RPGUNIT library ***') +
                          TOPGMQ(*PRV (*PGMBDY *NONE *NONE)) +
                          TOMSGQ(*TOPGMQ) MSGTYPE(*INFO)

             ENDPGM
