             /*  Create target library.                              */
             /* ---------------------------------------------------- */
             CHKOBJ     OBJ(&LIB) OBJTYPE(*LIB)
             MONMSG     MSGID(CPF9801) EXEC(DO)
               CRTLIB     LIB(&LIB) TEXT('*** in progress ***')
               RMVMSG     PGMQ(*SAME (*)) CLEAR(*ALL)
             GOTO       CMDLBL(haveLib)
             ENDDO

