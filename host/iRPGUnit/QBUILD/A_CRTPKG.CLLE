     /* =================================================================== */
     /*  RPGUnit - Create distribution package.                             */
     /* =================================================================== */
     /*  Updates the A_METADATA source member and saves the                 */
     /*  RPGUNIT library to save file RPGUNIT/RPGUNIT.                      */
     /* =================================================================== */
     /*   >>PRE-COMPILER<<                                                  */
     /*     >>CRTCMD<<  CRTBNDCL      PGM(&LI/&OB) +                        */
     /*                               SRCFILE(&SL/&SF)  +                   */
     /*                               SRCMBR(&SM);                          */
     /*     >>IMPORTANT<<                                                   */
     /*       >>PARM<< DBGVIEW(*LIST);                                      */
     /*       >>PARM<< DFTACTGRP(*NO);                                      */
     /*       >>PARM<< ACTGRP(*NEW);                                        */
     /*       >>PARM<< OPTION(*EVENTF);                                     */
     /*     >>END-IMPORTANT<<                                               */
     /*     >>EXECUTE<<                                                     */
     /*   >>END-PRE-COMPILER<<                                              */
     /* =================================================================== */
             PGM        PARM(&DEVLIB &I_PRDLIB &I_DBGVIEW)

             DCL        VAR(&DEVLIB    ) TYPE(*CHAR) LEN(10)
             DCL        VAR(&I_PRDLIB  ) TYPE(*CHAR) LEN(10)
             DCL        VAR(&I_DBGVIEW ) TYPE(*CHAR) LEN(10)

             DCL        VAR(&LIB       ) TYPE(*CHAR) LEN(10) VALUE('RPGUNIT')
             DCL        VAR(&SAVF      ) TYPE(*CHAR) LEN(10) VALUE('RPGUNIT')
             DCL        VAR(&TEXT      ) TYPE(*CHAR) LEN(50)
             DCL        VAR(&OBJD      ) TYPE(*CHAR) LEN(29) VALUE('iRPGUnit Utility')
             DCL        VAR(&OBJD_LEN  ) TYPE(*INT ) LEN( 4) VALUE(16)
             DCL        VAR(&RELEASE   ) TYPE(*CHAR) LEN(20)
             DCL        VAR(&TMPLIB    ) TYPE(*CHAR) LEN(10) VALUE('QTEMP')
             DCL        VAR(&OWNER     ) TYPE(*CHAR) LEN(10) VALUE('QPGMR')
             DCL        VAR(&DBGVIEW   ) TYPE(*CHAR) LEN(10) VALUE('NONE')
             DCL        VAR(&TGTRLS    ) TYPE(*CHAR) LEN(10) VALUE('V7R3M0')
             DCL        VAR(&IS_DEVLIB ) TYPE(*LGL )         VALUE('1')
             DCL        VAR(&TODAY     ) TYPE(*CHAR) LEN(10)

             DCL        VAR(&QBND      ) TYPE(*CHAR) LEN(10) VALUE('QBND')
             DCL        VAR(&QBUILD    ) TYPE(*CHAR) LEN(10) VALUE('QBUILD')
             DCL        VAR(&QCMD      ) TYPE(*CHAR) LEN(10) VALUE('QCMD')
             DCL        VAR(&QINCLUDE  ) TYPE(*CHAR) LEN(10) VALUE('QINCLUDE')
             DCL        VAR(&QLLIST    ) TYPE(*CHAR) LEN(10) VALUE('QLLIST')
             DCL        VAR(&QMISC     ) TYPE(*CHAR) LEN(10) VALUE('QMISC')
             DCL        VAR(&QPNLGRP   ) TYPE(*CHAR) LEN(10) VALUE('QPNLGRP')
             DCL        VAR(&QSRC      ) TYPE(*CHAR) LEN(10) VALUE('QSRC')
             DCL        VAR(&QSYSINC   ) TYPE(*CHAR) LEN(10) VALUE('QSYSINC')
             DCL        VAR(&QTESTCASES) TYPE(*CHAR) LEN(10) VALUE('QTESTCASES')
             DCL        VAR(&QUNITTEST ) TYPE(*CHAR) LEN(10) VALUE('QUNITTEST')

             DCL        VAR(&CCSID     ) TYPE(*CHAR) LEN( 4) VALUE('273')

             /* TODO: Remove this line at 31.12.2019 */
             DCL        VAR(&RPGUNIT1  ) TYPE(*CHAR) LEN(10) VALUE('RPGUNIT1')

             CHGVAR     VAR(&DEVLIB) VALUE(&DEVLIB)
             MONMSG     MSGID(MCH3601) EXEC(DO)
               RMVMSG     PGMQ(*SAME (*)) CLEAR(*ALL)
               SNDPGMMSG  MSGID(CPF9898) MSGF(*LIBL/QCPFMSG) +
                            MSGDTA('Usage: CALL PGM(A_CRTPKG) PARM(''devLibrary'')') +
                            TOPGMQ(*PRV (*PGMBDY *NONE *NONE)) +
                            TOMSGQ(*TOPGMQ) MSGTYPE(*ESCAPE)
             ENDDO

             CHGVAR     VAR(&LIB) VALUE(&I_PRDLIB)
             MONMSG     MSGID(MCH3601) EXEC(DO)
               RMVMSG     PGMQ(*SAME (*)) CLEAR(*ALL)
             ENDDO

             CHGVAR     VAR(&DBGVIEW) VALUE(&I_DBGVIEW)
             MONMSG     MSGID(MCH3601) EXEC(DO)
               RMVMSG     PGMQ(*SAME (*)) CLEAR(*ALL)
             ENDDO

