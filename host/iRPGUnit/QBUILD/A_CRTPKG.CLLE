             IF         COND(%sst(&TEXT 1 &OBJD_LEN) *NE &OBJD) THEN(DO)
               SNDPGMMSG  MSGID(CPF9898) MSGF(*LIBL/QCPFMSG) +
                            MSGDTA('Cannot use library' *BCAT &LIB *TCAT +
                                    '. Check object description:' *BCAT &TEXT ) +
                            TOPGMQ(*PRV (*PGMBDY *NONE *NONE)) +
                            TOMSGQ(*TOPGMQ) MSGTYPE(*ESCAPE)
             ENDDO

